                let reviews =  <%- JSON.stringify(reviews) %>;




                const review_list = document.getElementById('review_list');



                for (let review of reviews) {

                    let stars = '';
                    for (let i = 0; i < review.rating; i++) {
                        stars += `<img src="/assets/svg/illustrations/star.svg" alt="Review rating" width="16">`
                    }





                    let new_review = `

                        <div class="d-flex gap-1 mb-5 mt-3">
                            ${stars}

                        </div>



                        <!-- Media -->
                        <div class="d-flex align-items-center mb-3">
                            <div class="flex-shrink-0">
                                <img class="avatar avatar-sm avatar-circle" src="${review.user.profile_picture}"
                                    alt="Image Description">
                            </div>

                            <div class="flex-grow-1 ms-3">
                                <div class="d-flex justify-content-between align-items-center">
                                    <h5 class="mb-0">
                                        ${review.user.firstName + " " + review.user.lastName}
                                    </h5>
                                    <span class="d-block small text-muted">
                                        ${new String(review.date).substring(0, 16)}
                                    </span>
                                </div>
                            </div>
                        </div>
                        <!-- End Media -->

                        <div class="mb-5">
                            <p>
                                ${review.review}
                            </p>
                        </div>



                        <!-- Media -->
                       
                        <!-- End Media -->

                    <!--End Item-- >
                    `
                    review_list.innerHTML += new_review;
                }
            }






            //jquery reload review_list






        }

        async_call_reviews();




        more_reviews.addEventListener('click', async_call_reviews)






    </script>

</div>

