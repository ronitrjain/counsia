         img.onerror = function () {
             //set the background of the background div to empty
             console.log('error')
                background.style.background = '';
                background.style.backgroundColor = '#21325B';

         }

            img.src = image;

             function updateShareLink(imageURL, title) {
                    // Find the og:image and og:title meta tags
                    var ogImageTag = document.querySelector('meta[property="og:image"]');
                    var ogTitleTag = document.querySelector('meta[property="og:title"]');

                    // Update the content attribute of the og:image and og:title meta tags
                    if (ogImageTag) {
                        ogImageTag.setAttribute('content', imageURL);
                    }
                    if (ogTitleTag) {
                        ogTitleTag.setAttribute('content', title);
                    }
                }


            updateShareLink('<%= counselor.profile_picture %>', 'View <%= counselor.first_name %> on Counsia')

            //set og:url to the current url
            document.querySelector('meta[property="og:url"]').setAttribute('content', window.location.href);





</script>

   


