<% layout('./layouts/boilerplate') %>

    <!-- ========== MAIN CONTENT ========== -->
    <main id="content" role="main">
        <!-- Content -->
        <div class="container content-space-2 content-space-lg-3">
            <div class="w-md-75 w-lg-50 text-center mx-md-auto">
                <div class="mb-5">
                    <img class="img-fluid" src="/assets/svg/illustrations/oc-hi-five.svg" alt="Image Description"
                        style="width: 15rem;">
                </div>

                <div class="mb-5">
                    <% if (charges_enabled) { %>
                        <h1 class="h2">Your Stripe Connection Is Processing</h1>
                        <p>Check In Your Dashboard To Confirm Stripe Connection.</p>

                        <% }else{ %>

                            <h1 class="h2">Your Stripe Connection Is Processing</h1>
                            <p>Check In Your Dashboard To Confirm Stripe Connection.</p>

                            <% } %>


                </div>

                <a class="btn btn-primary btn-transition rounded-pill px-6" href="/dashboard/counselor">Go To
                    Mentor Dashboard
                </a>
            </div>
        </div>
        <!-- End Content -->